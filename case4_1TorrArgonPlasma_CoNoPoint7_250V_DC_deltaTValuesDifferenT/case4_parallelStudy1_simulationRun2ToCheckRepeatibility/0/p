FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

//internalField   uniform 13332.24;// 13332.24 Pa is approx 100 Torr.
internalField   uniform 133.3224;// 133.3224 Pa should be approx. 1 Torr.

boundaryField
{
    electrode
    {
        type            zeroGradient;
    }
    ground
    {
        type            zeroGradient;
    }
    otherfaces
    {
        type            empty;
    }
}


