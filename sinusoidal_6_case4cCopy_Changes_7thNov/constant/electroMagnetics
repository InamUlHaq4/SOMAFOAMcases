FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      electroMagnetics;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solutionDomain		plasma;

poissonEquationSolver	semiImplicit;

emcModel		power;

spatioTemporal 		oneD;

powerCoeffs
{
	mode 					continuousFrequencyModulated;
	initialAmplitude			200;//Table 3, 200 V corresponds tp 0.1 Torr//100;
	frequency 				13.56E6;
	bias					0;
	power 					3;//Why 3 ?density depends on power. Try 1, 0 etc and observe density response 0.5 etc
	dampingFactor				0.005;//Try 0, no need for daampin. Increases vlotage little bit
	waveform				sinusoidal;
	operation				symmetricalBipolar;
	//dutyCycle				0;//confirmed
	//naturalFrequency			0;
	//dampingRatio				0;
	tolerance				2.5;//?
	controlFrequency			1000;//13;//Why 13 ??must be integer value//0;//just copied naturalFrequency//TRy 1000, 1		0;//50 or 60 ?1000/13560000 = 74 microSec endTime approx.
	geometricFactor				14.93;//0;///just copied dampingRatio//Area/volume ratio		0;1/electrode distance for 1D case		
}

oneDCoeffs
{
	frequency		13.56E6;
	outputInterval	5;
	nCycles			2;
	deltaT			5E-11;	
}


