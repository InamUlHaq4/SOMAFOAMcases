FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      electron;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 5E16;

boundaryField
{
    ground
    {
        type    driftDiffusionElectronDensity;
        seec    0.1;
        Edepend true;
	    field_emission	false;
	    field_enhancement_factor 0;
	    work_function	0;
    }
    electrode
    {
        type    driftDiffusionElectronDensity;
        seec    0.1;
        Edepend true;
	    field_emission	false;
	    field_enhancement_factor 0;
	    work_function	0;
    }

    otherfaces
    {
        type        empty;
    }
}


